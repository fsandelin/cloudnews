{
  "name": "cloudnews",
  "description": "News in the cloud",
  "version": "1.0.0",
  "author": "carlssonochcarlsson",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev-prod": "cross-env NODE_ENV=production webpack-dev-server --open --hot",
    "dev-analyse": "cross-env NODE_ENV=production ANALYSE=true webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "build-windows": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "lint": "eslint --fix --ext .vue,.js src",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/*.{js,vue}",
      "src/components/**/*.{js,vue}",
      "src/helpers/*.{js,vue}",
      "src/store/**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "d3-geo": "^1.11.9",
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "npm": "^6.13.7",
    "socket.io-client": "^2.3.0",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.11",
    "vue-virtual-scroll-list": "^1.4.6",
    "vuex": "^3.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.1",
    "file-loader": "^5.1.0",
    "jest": "^25.1.0",
    "node-sass": "^4.12",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
